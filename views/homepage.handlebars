<!-- Loop over each object in the projects array -->
{{#each playlists as |playlist| }}
<div class="row mb-4 project">
  <div class="col-md-5">
    <h2>
      <a href="/playlist/{{playlist.id}}">{{platlist.title}}</a>
    </h2>
    <p>Created by {{playlist.user.name}} on {{ project.date_created}}</p>
